{
  "name": "react-email-resend",
  "scripts": {
    "dev": "wrangler dev src/index.tsx",
    "deploy": "wrangler deploy --minify src/index.tsx",
    "cf-typegen": "wrangler types --env-interface CloudflareBindings",
    "email": "email dev --dir ./src/default-email.tsx",
    "email-build": "email build --dir ./src/emails --packageManager bun"
  },
  "dependencies": {
    "@react-email/components": "^0.1.1",
    "@types/react": "^19.1.8",
    "hono": "^4.8.4",
    "react-email": "^4.0.17",
    "resend": "^4.6.0"
  },
  "devDependencies": {
    "wrangler": "^4.4.0"
  }
}